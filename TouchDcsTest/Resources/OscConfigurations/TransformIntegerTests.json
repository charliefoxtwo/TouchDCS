{
    "$schema": "https://raw.githubusercontent.com/charliefoxtwo/TouchDCS/main/Configuration/Schema/LayoutConfiguration.json.schema",

    "syncAddress": "TransformIntegerTests",
    
    "properties": {
        "TransformedIntOutMultiply": {
            "transform": {
                "multiply": 2
            }
        },
        "TransformedIntOutDivide": {
            "transform": {
                "divide": 2
            }
        },
        "TransformedIntOutAdd": {
            "transform": {
                "add": 2
            }
        },
        "TransformedIntOutSubtract": {
            "transform": {
                "subtract": 2
            }
        },
        "TransformedIntOutForceDecimal": {
            "transform": {
                "forceDecimalPlaces": 2
            }
        }
    }
}